<h2>Sign In</h2>
<div class="sign_in">
  <form action="<%= session_url %>" method="post"> 
    <%= auth_token_input %>
    

    <label>
      Username
      <input type="text" name="user[username]" value="">
    </label>

    <label>
      Password
      <input type="password" name="user[password]" value="">
    </label>

    <label>
      <input type="submit" value="Sign In">
    </label>
  </form>
</div>