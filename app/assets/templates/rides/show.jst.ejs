<div id="new-map" class="col-xs-12 container-fluid">
	<div class="form">
		<div class="form_input">
			<div><%=ride.get('title')%></div>
		</div>
		<div class="form_input">
			<div><%=ride.get('date')%></div>
		</div>
		<a class="link_button" href="#rides/edit/<%=ride.id%>">Edit</a>
		<div class="info">Feet of climbing: <%=ride.get('elevation')%></div>
		<div class="info">Total miles: <%=ride.get('distance')%></div>
		
	</div>
	<div class="row" id="map">
		<div id="map-canvas"></div>
		<div id="data">
			<div id="elevation_chart"></div>
		</div>
		<div id="directions-hover" class="link_button">Directions</div>
		<div id="directions-panel"></div>
	</div>
</div>


